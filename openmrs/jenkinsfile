node{
    stage('SCM'){
        //git clone
        git 'https://github.com/openmrs/openmrs-core.git'
     }
    stage('build the packages'){
        //mvn package
        sh label: '', script: 'mvn package'
    }
    stage('archival'){
        //archiving artifacts
        archiveArtifacts 'target/*.jar'
    }
    stage('junit tests'){
        //junit tests reports
        junit 'target/surefire-reports/*.xml'
    }
}